{
  "name": "Session Clear",
  "version": "1.0.0",
  "description": "An extension to open a new tab",
  "permissions": [
    "tabs",
    "activeTabs",
    "session",
    "cookies",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "author": [
    "Kumud Kharel",
    "Angel Dhakal"
  ]
}